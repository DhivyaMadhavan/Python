#merge the array such that first array is in ascending order and second one in descending order.
#program
n,m = input().split()
A = []
B = []
for i in range(1):
   l = list(map(int,input().split()))
   A.append(l)
A[0].sort()
for i in range(1):
   l = list(map(int,input().split()))
   B.append(l)
B[0].sort(reverse=True)    
new = A[0]+B[0]
print(*new)
